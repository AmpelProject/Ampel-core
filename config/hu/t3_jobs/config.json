[{
	"_id": "Slack_publish_1",
	"active": false,
	"schedule": {
		"mode": "fixed_rate",
		"interval": 1
	},
	"onError": {
		"sendMail": "ztf-software@desy.de",
		"stopAmpel": false,
		"retry": false
	},
	"transients": {
		"select": {
			"created": {
				"timedelta": {
					"days": -40
				}
			},
			"modified": {
				"timedelta": {
					"days": -1
				}
			},
			"channels": [
				"HU_SN1",
				"HU_SN2"
			],
			"withFlags": "INST_ZTF",
			"withoutFlags": "HAS_ERROR"
		},
		"load": {
			"state": "latest",
			"docs": [
				"TRANSIENT",
				"COMPOUND",
				"T2RECORD"
			],
			"t2s": [
				"SNCOSMO",
				"AGN"
			]
		},
		"chunk": 200
	},
	"tasks": [
		{
			"t3Unit": "SLACKPUBLISHER",
			"runConfig": "hu_sp_1",
			"subSel": {
				"channel": "HU_SN1",
				"state": "latest",
				"t2Result": "SNCOSMO"
			}
		},
		{
			"t3Unit": "SLACKPUBLISHER",
			"runConfig": "hu_sp_2",
			"subSel": {
				"channel": "HU_SN2",
				"state": "latest",
				"t2": "AGN"
			}
		}
	]
},
