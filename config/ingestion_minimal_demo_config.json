{
    "channels": {
        "HU_SN1": {
            "version": 1.0,
            "active": false,
            "sources": {
                "ZTFIPAC": {
                    "parameters": {
                        "ZTFPartner": true,
                        "autoComplete": true,
                        "updatedHUZP": false
                    },
                    "t0Filter": {
                        "dbEntryId": "BasicFilter",
                        "runConfig": {
                            "criteria": [
                                {
                                    "attribute": "candid",
                                    "value": null,
                                    "operator": "is not"
                                },
                                {
                                    "attribute": "rb",
                                    "value": 0.6,
                                    "operator": ">"
                                },
                                {
                                    "attribute": "mag",
                                    "value": 21,
                                    "operator": "<"
                                }
                            ],
                            "len": 3,
                            "operator": ">"
                        }
                    },
                    "t2Compute": [
                        {
                            "t2Unit": "SNCOSMO",
                            "runConfig": "default"
                        }
                    ]
                }
            }
        }
    },
    "global": {
        "photoPoints": {
            "ZTFIPAC": {
                "alFlags": "INST_ZTF|PP_IPAC",
                "dictKeywords": {
                    "tranId": "alertid",
                    "pptId": "candid",
                    "obsDate": "jd",
                    "filterId": "fid",
                    "mag": "magpsf"
                },
                "stamperClass": "ZIPhotoPointStamper",
                "formats": {
                    "date": "julian"
                }
            }
        },
        "alertIngestion": {
            "source": {
                "ZTFIPAC": {
                    "checkReprocessing": true,
                    "alertHistoryLength": 30
                }
            }
        }
    },
    "t0_filters": {
        "BasicFilter": {
            "classFullPath": "ampel.pipeline.t0.filters.BasicFilter"
        }
    },
    "t0_sources": {
        "ZTFIPAC": {
            "alerts": {
                "processing": {
                    "parser": "ampel.pipeline.t0.alerts.ZIAlertParser",
                    "deserializer": "ampel.pipeline.t0.alerts.AvroDeserializer"
                },
                "alertFlags": [
                    "INST_ZTF",
                    "SRC_IPAC"
                ],
                "mappings": {
                    "tranId": "objectId",
                    "pptId": "candid",
                    "obsDate": "jd",
                    "filterId": "fid",
                    "mag": "magpsf"
                }
            },
            "ingestion": {
                "class": "ampel.pipeline.t0.ingesters.ZIAlertIngester",
                "photoPointShaper": "ampel.pipeline.t0.ingesters.ZIPhotoPointShaper",
                "flags": {
                    "photoPoint": "INST_ZTF",
                    "transient": "INST_ZTF"
                },
                "tranIdPrefix": null
            }
        }
    },
    "t2_run_config": {
        "SNCOSMO_default": {
            "t2Unit": "SNCOSMO",
            "runConfig": "default",
            "author": "ampel-info@desy.de",
            "version": 1.0,
            "lastChange": "18.03.2018",
            "private": false,
            "parameters": {
                "model": "salt2"
            }
        }
    },
    "t2_units": {
        "SNCOSMO": {
            "classFullPath": "ampel.contrib.hu.t2.T2SNCosmo",
            "version": 1.0,
            "private": false,
            "upperLimits": true,
            "description": {
                "title": "SALT2 template fitting",
                "details": [
                    "This module fits lightcurves using SNCOSMO",
                    "More details later"
                ]
            }
        }
    }
}