{
	"t0": {
		"unit": {
			"controller": "ampel.t0.T0Controller",
			"processor": [
				"ampel.ingest.PointT2Ingester"
			]
		}
	},
	"t2": {
		"unit": {
			"base": [
				"ampel.test.unit.DemoPointT2Unit",
				"ampel.test.unit.DemoFirstPointT2Unit"
			]
		}
	},
	"t3": {
		"unit": {
			"controller": "ampel.t3.T3Controller",
			"processor": [
				"ampel.t3.T3DefaultProcessor",
				"ampel.t3.load.T3LatestStateDataLoader",
				"ampel.t3.select.T3DefaultStockSelector"
			],
			"aux": [
				"ampel.t3.run.filter.SnapDataArrayFilter"
			]
		},
		"alias": {
			"%TRANSIENT": {"col": "stock"},
			"%DATAPOINT": {"col": "t0"},
			"%COMPOUND": {"col": "t1"},
			"%T2RECORD": {"col": "t2"},
			"%LOGRECORD": {"col": "logs"}
		}
	},
	"aux": [
		"ampel.utils.filter.SimpleDictArrayFilter"
	],
	"template": {
		"legacy_simple_process": "ampel.model.template.LegacyT3Process1"
	},
    "resource": {
        "%mongo": {
            "writer": "mongodb://localhost:27017",
            "logger": "mongodb://localhost:27017"
        },
        "%extcats": {
            "reader": "mongodb://localhost:27017",
            "writer": "mongodb://localhost:27017"
        }
    }
}
